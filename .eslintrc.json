{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "camelcase": 0,
      "object-curly-newline": 0,
      "import/no-extraneous-dependencies": 0
    }
}
